    <rules>
        <fullName>Neg-Arb-22-Update Arbitrator Accept Date</fullName>
        <actions>
            <name>Arbitrator_Accept_Date</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Arbitrator_Accept_Decline__c</field>
            <operation>equals</operation>
            <value>Accept</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Neg-Arbitrating-1-Update Arbitrator Email</fullName>
        <actions>
            <name>Arbitrator_Contact_Email</name>
            <type>FieldUpdate</type>
