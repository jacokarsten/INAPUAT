        <fullName>Neg-Neg-6-Item Flagged for Arbitration Email</fullName>
        <actions>
            <name>Refer_to_Arbitration_Accept_or_Decline</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Refer_to_Arbitration_Count__c</field>
            <operation>greaterOrEqual</operation>
            <value>1</value>
        </criteriaItems>
        <description>Email notification to both parties</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Neg-Settlement-1-Payment Obligation Notification</fullName>
        <actions>
            <name>Payment_Obligation_Email</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Checkbox_Update_Email_Sent</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>(1 OR 2 OR 3) and 4 and 5</booleanFilter>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Settled</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Arbitrated</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Status</field>
            <operation>equals</operation>
            <value>Partial Settlement</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Claimant_Confirms_Case_Settled__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Stage__c</field>
            <operation>equals</operation>
            <value>Finalize</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
