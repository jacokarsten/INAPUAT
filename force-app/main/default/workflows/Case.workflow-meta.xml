        <active>true</active>
        <description>Updates the email address on the case when the Arbitrator contact is added to the case.</description>
        <formula>Arbitrator_Contact__c != NULL</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
