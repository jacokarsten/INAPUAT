        <actions>
            <name>Facilitator_Case_Notification_email</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Facilitator_Email__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
