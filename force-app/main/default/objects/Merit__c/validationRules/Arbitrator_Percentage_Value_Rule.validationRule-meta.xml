<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Arbitrator_Percentage_Value_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>OR(
 Arbitrator_Award__c &gt; 1.0,
	Arbitrator_Award__c &lt; 0.0
)</errorConditionFormula>
    <errorDisplayField>Arbitrator_Award__c</errorDisplayField>
    <errorMessage>Please enter a value between 0 - 100</errorMessage>
</ValidationRule>
